{
  "name": "techjeed/cashier",
  "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
  "keywords": [
    "laravel",
    "paystack",
    "billing",
    "cashier"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abdulmajid Yunusa",
      "email": "techjeed@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "dompdf/dompdf": "^0.8.0",
    "illuminate/database": "^8.80.0",
    "illuminate/support": "^8.80.0",
    "nesbot/carbon": "^1.26.3 || ^2.0",
    "symfony/http-kernel": "^5.4.0",
    "unicodeveloper/laravel-paystack": "^1.0"
  },
  "require-dev": {
    "illuminate/http": "^8.80.0",
    "illuminate/routing": "^8.80.0",
    "illuminate/view": "^8.80.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "techjeed\\Cashier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "techjeed\\Cashier\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "techjeed\\Cashier\\CashierServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
