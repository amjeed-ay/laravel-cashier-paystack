{
  "name": "techjeed/cashier",
  "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
  "keywords": [
    "laravel",
    "paystack",
    "billing",
    "cashier"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abdulmajid Yunusa",
      "email": "techjeed@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "dompdf/dompdf": "^0.8.0",
    "illuminate/database": "~5.7.0|~5.8.0",
    "illuminate/support": "~5.7.0|~5.8.0",
    "nesbot/carbon": "^1.26.3 || ^2.0",
    "symfony/http-kernel": "~4.0",
    "unicodeveloper/laravel-paystack": "^1.0"
  },
  "require-dev": {
    "illuminate/http": "~5.7.0|~5.8.0",
    "illuminate/routing": "~5.7.0|~5.8.0",
    "illuminate/view": "~5.7.0|~5.8.0",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "techjeed\\Cashier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "techjeed\\Cashier\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "techjeed\\Cashier\\CashierServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
